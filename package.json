{
  "name": "conjure-svelte",
  "version": "0.0.1",
  "scripts": {
    "dev": "run-p -r dev:serve check:watch lint:watch",
    "dev:serve": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "pnpm run check -- --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "lint:watch": "chokidar -c \"eslint . --ext .ts,.svelte --cache\" --silent --initial \"src/**/*.svelte\" \"src/**/*.ts\"",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@felte/common": "^1.0.3",
    "@felte/reporter-svelte": "^1.0.3",
    "@felte/validator-zod": "^1.0.5",
    "felte": "^1.1.5",
    "zod": "^3.14.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@iconify-json/ri": "^1.1.1",
    "@sveltejs/adapter-static": "^1.0.0-next.29",
    "@sveltejs/kit": "^2.0.6",
    "@svelteness/kit-docs": "^0.18.4",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "autoprefixer": "^10.4.4",
    "chokidar-cli": "^3.0.0",
    "clsx": "^1.1.1",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "husky": "^7.0.4",
    "import-sort-style-module": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "prettier-plugin-import-sort": "^0.0.7",
    "prettier-plugin-svelte": "^2.6.0",
    "shiki": "^0.10.1",
    "svelte": "^3.46.6",
    "svelte-check": "^2.4.6",
    "svelte-preprocess": "^4.10.5",
    "svelte2tsx": "^0.5.6",
    "tailwindcss": "^3.0.23",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3",
    "unplugin-icons": "^0.14.1"
  },
  "importSort": {
    ".js, .cjs, .ts, .svelte": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "type": "module",
  "engines": {
    "node": ">= 18.0.0"
  }
}
